{

  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2018",
    "module": "commonjs",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
    ],
    "noEmit": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "experimentalDecorators": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
